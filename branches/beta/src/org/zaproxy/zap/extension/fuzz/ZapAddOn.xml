<zapaddon>
    <name>AdvFuzzer</name>
    <version>2</version>
    <semver>1.0.1</semver>
    <description>Advanced fuzzer for manual testing</description>
    <author>ZAP Dev Team</author>
    <url>https://code.google.com/p/zap-extensions/</url>
    <changes>
    <![CDATA[
    Fix the help button in the Fuzzer dialogue and broken link in Fuzzer dialogue help page.
    ]]>
    </changes>
    <extensions>
        <extension>org.zaproxy.zap.extension.fuzz.ExtensionFuzz</extension>
        <extension>org.zaproxy.zap.extension.fuzz.httpfuzzer.ExtensionHttpFuzzer</extension>
    </extensions>
    <filters/>
    <files>
        <file>scripts/templates/httpfuzzerprocessor/HTTP Fuzzer Processor default template.js</file>
        <file>scripts/templates/payloadgenerator/Payload Generator default template.js</file>
        <file>scripts/templates/payloadprocessor/Payload Processor default template.js</file>
    </files>
    <not-before-version>2.4.0</not-before-version>
    <not-from-version/>
</zapaddon>
